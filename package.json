{
    "name": "colorado-theme",
    "displayName": "Colorado Theme",
    "description": "A chilled theme for Visual Studio Code",
    "version": "0.0.15",
    "publisher": "StevenJBorik",
    "repository": {
        "type": "git",
        "url": "https://github.com/StevenJBorik/colorado-theme"
    },
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Colorado",
                "uiTheme": "vs",
